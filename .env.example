# Database Configuration - PostgreSQL
POSTGRES_HOST=your-postgres-host
POSTGRES_PORT=5432
POSTGRES_USER=your-postgres-user
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DATABASE=your-database
POSTGRES_SCHEMA=your-schema

# Database Configuration - SQL Server
SQLSERVER_HOST=your-sqlserver-host.database.windows.net
SQLSERVER_PORT=1433
SQLSERVER_DATABASE=your-database
SQLSERVER_USER=your-sqlserver-user
SQLSERVER_PASSWORD=your-sqlserver-password
SQLSERVER_SCHEMA=graph

# Meta/Facebook Graph API Configuration
# Add your Graph API tokens separated by comma
# Example: token1,token2,token3
GRAPH_API_TOKENS=your-token-1,your-token-2,your-token-3

# Meta Ads Account Configuration
# Format: account_id:table_name (separated by comma)
# Example: 123456789:bm_01,987654321:bm_02
META_ACCOUNTS=account_id_1:bm_01,account_id_2:bm_02

# API Configuration
GRAPH_API_VERSION=v19.0
DATA_RETENTION_DAYS=15
API_REQUEST_TIMEOUT=30
RATE_LIMIT_SLEEP_SECONDS=60

# Airflow Configuration
AIRFLOW_OWNER=data-engineering
AIRFLOW_RETRIES=2
AIRFLOW_RETRY_DELAY_MINUTES=5
